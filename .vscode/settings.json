// Place your settings in this file to overwrite default and user settings.
{
  "editor.codeActionsOnSave": {
    "source.fixAll.tslint": true
  },
  "editor.formatOnSave": true,
  "editor.formatOnType": false,
  "files.eol": "\n",
  "cSpell.words": [
    "Parens",
    "Whitespaces",
    "codebases",
    "commonmark",
    "docblock",
    "editorconfig",
    "esbenp",
    "gitignore",
    "junit",
    "prettier's",
    "prettiercode",
    "prettierignore",
    "prettierrc",
    "promisify",
    "stylelint",
    "vsce"
  ]
}
